-%><%
let helpBlocks = 0; %>
{
    "<%= angularAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityClassPluralHumanized %>",
                "createLabel": "Създай нов <%= entityClassHumanized %>",
                "createOrEditLabel": "Създай или модифицирай <%= entityClassHumanized %>"<% if (searchEngine === 'elasticsearch') { %>,
                "search": "Търси <%= entityClassHumanized %>"<% } %>
            },<% if (!microserviceAppName) { %>
            "created": "Създаден е нов <%= entityClassHumanized %> с идентификатор {{ id }}",
            "updated": "<%= entityClassHumanized %> с идентификатор {{ id }} е модифициран",
            "deleted": "<%= entityClassHumanized %> с идентификатор {{ id }} е изтрит",<% } %>
            "delete": {
                "question": "Сигурни ли сте, че искате да изтриете <%= entityClassHumanized %> {{ id }}?"
            },
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) {
            if (typeof fields[idx].javadoc !== 'undefined') ++helpBlocks; %>,
            "<%=fields[idx].fieldName%>": "<%= fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { 
            	let relationshipName = relationships[idx].relationshipName;
            	let relationshipNameHumanized = relationships[idx].relationshipNameHumanized;
            	
            	if (relationshipType === 'many-to-many' || relationshipType === 'one-to-many') {
            	  relationshipName = relationships[idx].relationshipNameCapitalizedPlural;
       	          relationshipNameHumanized = relationships[idx].relationshipNameHumanizedPlural;
//       	          const relationshipNameCapitalized = relationships[idx].relationshipNameCapitalized;
            	}
            %>,
            "<%=relationshipName%>": "<%= relationshipNameHumanized %>"<% } if (helpBlocks > 0){%>,
            "help": {<% for (idx in fields) {
                if (fields[idx].javadoc) {
                    --helpBlocks; %>
                "<%=fields[idx].fieldName%>": "<%= fields[idx].javadoc %>"<% if (helpBlocks > 0) { %>,<%}
                }
            } %>
            }<% } %>
        }
    }<% if (microserviceAppName) { %>,
    "<%= microserviceAppName %>": {
        "<%= entityTranslationKey %>" : {
            "created": "Създаден е нов <%= entityClassHumanized %> с идентификатор {{ id }}",
            "updated": "<%= entityClassHumanized %> с идентификатор {{ id }} е модифициран",
            "deleted": "<%= entityClassHumanized %> с идентификатор {{ id }} е изтрит"
        }
    }<% } %>
}
